function wcGetCst(pszWord : PChar; var bSize : BYTE; fErrMsg : BOOL) : DWORD; export;
var	bCurMap,
		bCurCst	: BYTE;
		cMap		: Char;
		fFind		: Boolean;
		szVal		: Array[0..255]of char;
begin

	{ Cherche une table qui correspond }
	cMap := pszWord[0];
	fFind := FALSE;
	bCurMap := aFirstCst;
	while (bCurMap <= aLastCst) AND NOT fFind do begin
		if ((aCstMap[bCurMap].cMap = '*') OR (cMap = aCstMap[bCurMap].cMap)) AND
			 ((bSize = BYTE(-1)) OR (bSize = aCstMap[bCurMap].bSize)) then begin
			bCurCst := aCstMap[bCurMap].bFirst;
			while (bCurCst <= aCstMap[bCurMap].bLast) AND NOT fFind do begin
				case aCstMap[bCurMap].bSize of
					2 :	if (StrComp(pszWord, TwMap(aCstMap[bCurMap].pMap^)[bCurCst].tok) = 0) then begin
								{wvsprintf(szVal, '%x', TwMap(aCstMap[bCurMap].pMap^)[bCurCst].wCst);
								MessageBox(0, TwMap(aCstMap[bCurMap].pMap^)[bCurCst].tok, szVal, 0);}
								fFind := TRUE;
							end;
					4 : if (StrComp(pszWord, TdwMap(aCstMap[bCurMap].pMap^)[bCurCst].tok) = 0) then begin
								{wvsprintf(szVal, '%lx', TdwMap(aCstMap[bCurMap].pMap^)[bCurCst].dwCst);
								MessageBox(0, TdwMap(aCstMap[bCurMap].pMap^)[bCurCst].tok, szVal, 0);}
								fFind := TRUE;
							end;
					else if	(StrComp(pszWord, TszMap(aCstMap[bCurMap].pMap^)[bCurCst].tok) = 0) then begin
								{wvsprintf(szVal, '%s', TszMap(aCstMap[bCurMap].pMap^)[bCurCst].szCst);
								MessageBox(0, TszMap(aCstMap[bCurMap].pMap^)[bCurCst].tok, szVal, 0);}
								fFind := TRUE;
							end;
				end;
				if NOT fFind then inc(bCurCst);
			end;
		end;
		if NOT fFind then inc(bCurMap);
	end;

	if fFind then begin
		bSize := aCstMap[bCurMap].bSize;
		case bSize of
			2 :	wcGetCst := DWORD(TwMap(aCstMap[bCurMap].pMap^)[bCurCst].wCst);
			4 : wcGetCst := TdwMap(aCstMap[bCurMap].pMap^)[bCurCst].dwCst;
			else wcGetCst := DWORD(TszMap(aCstMap[bCurMap].pMap^)[bCurCst].szCst);
		end;
	end else begin
		{ erreur de syntaxe }
		if fErrMsg then MessageBox(GetActiveWindow, pszWord, 'WinCst syntax error !!!', MB_OK OR MB_ICONSTOP OR MB_APPLMODAL);
		wcGetCst := DWORD(-1);
	end;
end;

procedure FindCst(pszWord : PChar; dwVal : DWORD; pszRet : PCHAR; nSize : integer; var bSize : BYTE; fErrMsg : BOOL); export;
var	bCurMap,
		bCurCst	: BYTE;
		cMap		: Char;
		fFind		: Boolean;
		szVal		: Array[0..255]of char;
begin

	{ Cherche une table qui correspond }
	cMap := pszWord[0];
	fFind := FALSE;
	bCurMap := aFirstCst;
	while (bCurMap <= aLastCst) AND NOT fFind do begin
		if ((aCstMap[bCurMap].cMap = '*') OR (cMap = aCstMap[bCurMap].cMap)) AND
			 ((bSize = BYTE(-1)) OR (bSize = aCstMap[bCurMap].bSize)) then begin
			bCurCst := aCstMap[bCurMap].bFirst;
			while (bCurCst <= aCstMap[bCurMap].bLast) AND NOT fFind do begin
				case aCstMap[bCurMap].bSize of
					2 :	if (StrPos(TwMap(aCstMap[bCurMap].pMap^)[bCurCst].tok, pszWord) <> nil) AND
								 (TwMap(aCstMap[bCurMap].pMap^)[bCurCst].wCst = dwVal) then begin
								fFind := TRUE;
							end;
					4 : if (StrPos(TdwMap(aCstMap[bCurMap].pMap^)[bCurCst].tok, pszWord) <> nil) AND
								 (TdwMap(aCstMap[bCurMap].pMap^)[bCurCst].dwCst = dwVal) then begin
								fFind := TRUE;
							end;
					else if	(StrPos(TszMap(aCstMap[bCurMap].pMap^)[bCurCst].tok, pszWord) <> nil) AND
									(StrComp(TszMap(aCstMap[bCurMap].pMap^)[bCurCst].szCst, MAKEINTRESOURCE(dwVal)) = 0) then begin
								fFind := TRUE;
							end;
				end;
				if NOT fFind then inc(bCurCst);
			end;
		end;
		if NOT fFind then inc(bCurMap);
	end;

	if fFind then begin
		bSize := aCstMap[bCurMap].bSize;
		case bSize of
			2 :	StrLCopy(pszRet, TwMap(aCstMap[bCurMap].pMap^)[bCurCst].tok, nSize);
			4 : StrLCopy(pszRet, TdwMap(aCstMap[bCurMap].pMap^)[bCurCst].tok, nSize);
			else StrLCopy(pszRet, TszMap(aCstMap[bCurMap].pMap^)[bCurCst].tok, nSize);
		end;
	end else begin
		{ erreur de syntaxe }
		if fErrMsg then MessageBox(GetActiveWindow, pszWord, 'WinCst syntax error !!!', MB_OK OR MB_ICONSTOP OR MB_APPLMODAL);
		StrLCopy(pszRet, 'UNKNOWN !', nSize);
	end;
end;

